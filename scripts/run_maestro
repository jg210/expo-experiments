#!/bin/bash

set -o errexit
set -o nounset
set -o pipefail

SOURCE_DIR="$(readlink -f "$(dirname "${BASH_SOURCE[0]}")"/..)"
# shellcheck source=.common.sh
. "${SOURCE_DIR}/.common.sh"
# shellcheck source=environment
. "${SOURCE_DIR}/environment"
log "starting maestro"
exec maestro test e2e/maestro/*.yaml